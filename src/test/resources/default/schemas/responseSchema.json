{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "filesArchived",
    "date"
  ],
  "properties": {
    "filesArchived": {
      "$id": "#/properties/filesArchived",
      "type": "integer",
      "title": "The Filesarchived Schema",
      "default": 0,
      "examples": [
        3
      ]
    },
    "date": {
      "$id": "#/properties/date",
      "type": "string",
      "title": "The Date Schema",
      "default": "",
      "examples": [
        "21-03-2019"
      ],
      "pattern": "^(.*)$"
    }
  }
}